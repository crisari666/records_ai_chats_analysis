# Ollama Performance Configuration
# Add these variables to your .env file

# Ollama server host (use remote server if you have one)
OLLAMA_HOST=http://localhost:11434

# Request timeout in milliseconds (90 seconds for slower hardware)
OLLAMA_TIMEOUT=90000

# Maximum number of messages to analyze per conversation
# Lower values = faster processing but less context
# Recommended: 30-50 for VPS with 16GB RAM
MAX_MESSAGES_FOR_ANALYSIS=30
